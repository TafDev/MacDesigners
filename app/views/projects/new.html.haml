%section
  .container
    .row
      .col-lg-12.text-center
        .h1
          New Project
        = form_for @project, class: "form-group" do |f|
          = f.label :title
          = f.text_field :title
          %br
          = f.label :description
          = f.text_field :description
          %br
          = f.label :image
          = f.file_field :image
          %br
          = f.submit